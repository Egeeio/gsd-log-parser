{
  "name": "gsd-log-parser",
  "version": "1.0.0",
  "description": "a journald log parser",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js minecraft",
    "restart": "npm run clean-dist && npm run build && npm run start",
    "build": "node_modules/.bin/tsc --build tsconfig.json",
    "rebuild": "npm run clean-dist && npm run build",
    "clean": "rm -rf node_modules && npm run clean-dist",
    "clean-npm": "rm -rf node_modules && npm install",
    "clean-dist": "rm -rf dist/* && touch dist/.gitkeep",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Egeeio/gsd-log-parser.git"
  },
  "author": "egee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Egeeio/gsd-log-parser/issues"
  },
  "homepage": "https://github.com/Egeeio/gsd-log-parser#readme",
  "dependencies": {
    "ioredis": "^4.10.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.11",
    "@types/node": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.8.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0"
  }
}
